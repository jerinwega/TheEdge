{
  "expo": {
    "name": "TheEdge",
    "slug": "TheEdge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "theedge",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
          "backgroundColor": "#0D0D0D",
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jerinwega1515.TheEdge"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.jerinwega1515.TheEdge"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#0D0D0D",
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "dark": {
            "image": "./assets/images/splash-icon.png",
            "backgroundColor": "#0D0D0D"
          },
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
